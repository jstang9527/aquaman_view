lue: 59, name: 'Forecasts' }\n          ]\n        }\n      }\n    }\n  },\n  data() {\n    return {\n      chart: null\n    }\n  },\n  watch: {\n    chartData: {\n      deep: true,\n      handler(val) {\n        this.initChart()\n      }\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.initChart()\n    })\n  },\n  beforeDestroy() {\n    if (!this.chart) {\n      return\n    }\n    this.chart.dispose()\n    this.chart = null\n  },\n  methods: {\n    initChart() {\n      this.chart = echarts.init(this.$el, 'macarons')\n\n      this.chart.setOption({\n        title: {\n          text: this.chartData.title,\n          textStyle: {\n            fontSize: 15\n          }\n        },\n        tooltip: {\n          trigger: 'item',\n          formatter: '{a} <br/>{b} : {c} ({d}%)'\n        },\n        legend: {\n          left: 'center',\n          bottom: '10',\n          data: this.chartData.legend\n        },\n        series: [\n          {\n            name: '服务占比',\n            type: 'pie',\n            top: 20,\n            // roseType: 'radius',\n            radius: '55%',\n            center: ['50%', '45%'],\n            data: this.chartData.series,\n            animationEasing: 'cubicInOut',\n            animationDuration: 2600\n          }\n        ]\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/dashboard/components"}]}