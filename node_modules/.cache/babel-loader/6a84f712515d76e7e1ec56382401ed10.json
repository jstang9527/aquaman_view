A,QAAA,QAAA,EAAA,CAAA,QAAA,EAAA,OAAA,CAJA;AAKA,QAAA,SAAA,EAAA,CALA;AAMA,QAAA,OAAA,EAAA,CAAA,KAAA,CANA;AAOA,QAAA,MAAA,EAAA,WAPA;AAQA,QAAA,MAAA,EAAA,CAAA,eAAA,EAAA,eAAA,CARA;AASA,QAAA,SAAA,EAAA,aATA;AAUA,QAAA,QAAA,EAAA,CAAA,MAAA,EAAA,OAAA,CAVA;AAWA,QAAA,UAAA,EAAA;AAXA,OADA,CAHA;AAkBA,MAAA,MAAA,EAAA,KAlBA;AAmBA,MAAA,UAAA,EAAA,EAnBA;AAoBA,MAAA,KAAA,EAAA,CApBA;AAqBA,MAAA,WAAA,EAAA,KArBA;AAsBA,MAAA,SAAA,EAAA;AACA,QAAA,OAAA,EAAA,CADA;AACA,QAAA,SAAA,EAAA,EADA;AACA,QAAA,IAAA,EAAA;AADA,OAtBA,CAwBA;;AAxBA,KAAA;AA0BA,GApCA;AAqCA,EAAA,OArCA,qBAqCA;AACA,SAAA,OAAA;AACA,GAvCA;AAwCA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,OAFA,qBAEA;AAAA;;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,MAAA,QAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,UAAA,CAAA,YAAA;AAAA,UAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AAAA,SAAA,EAAA,MAAA,IAAA,CAAA;AACA,OAJA;AAKA,KAVA;AAWA;AACA,IAAA,YAZA,0BAYA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,CAAA;AACA,WAAA,OAAA;AACA,KAfA;AAgBA;AACA,IAAA,YAjBA,wBAiBA,GAjBA,EAiBA,KAjBA,EAiBA;AAAA;;AACA,WAAA,QAAA,CAAA,oCAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,KAAA,EADA,CAEA;AACA;AACA;AACA;AACA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,KAAA,EAAA,SADA;AACA,UAAA,OAAA,EAAA,OADA;AACA,UAAA,IAAA,EAAA,SADA;AACA,UAAA,QAAA,EAAA;AADA,SAAA;AAGA,OAdA,EAcA,KAdA,CAcA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,KAAA,EAAA,QADA;AACA,UAAA,OAAA,EAAA,eADA;AACA,UAAA,IAAA,EAAA,MADA;AACA,UAAA,QAAA,EAAA;AADA,SAAA;AAGA,OAlBA;AAmBA,KArCA;AAsCA;AACA,IAAA,YAvCA,wBAuCA,KAvCA,EAuCA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,CAAA,KAAA,CAAA;AACA,WAAA,UAAA,GAAA;AAAA,QAAA,GAAA,EAAA,IAAA,CAAA,GAAA;AAAA,QAAA,IAAA,EAAA,IAAA,CAAA,IAAA;AAAA,QAAA,IAAA,EAAA,IAAA,CAAA,IAAA;AACA,QAAA,QAAA,EAAA,IAAA,CAAA,QAAA,CAAA,QAAA,EADA;AACA,QAAA,SAAA,EAAA,IAAA,CAAA,SADA;AAEA,QAAA,OAAA,EAAA,IAAA,CAAA,OAAA,CAAA,QAAA,EAFA;AAEA,QAAA,MAAA,EAAA,IAAA,CAAA,MAFA;AAGA,QAAA,MAAA,EAAA,IAAA,CAAA,MAAA,CAAA,QAAA,EAHA;AAGA,QAAA,SAAA,EAAA,IAAA,CAAA,SAHA;AAIA,QAAA,QAAA,EAAA,IAAA,CAAA,QAAA,CAAA,QAAA,EAJA;AAIA,QAAA,UAAA,EAAA,IAAA,CAAA;AAJA,OAAA;AAMA,WAAA,MAAA,GAAA,IAAA;AACA;AAhDA;AAxCA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"filter-container\">\n      <el-input v-model=\"listQuery.info\" placeholder=\"TaskName\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"handleFilter\" />\n      <el-button v-waves class=\"filter-item\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleFilter\">Search</el-button>\n      <router-link :to=\"'/auth_tester/task_create'\">\n        <el-button class=\"filter-item\" style=\"margin-left: 10px;\" type=\"primary\" icon=\"el-icon-edit\">Add</el-button>\n      </router-link>\n    </div>\n    <!--弹出框-->\n    <el-dialog title=\"任务详情\" :visible.sync=\"dialog\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"dialogData\" label-position=\"left\" label-width=\"120px\" size=\"mini\" style=\"width: 85%; margin-left:50px;\">\n          <el-form-item label=\"任务ID\"> <el-tag>{{dialogData._id}}</el-tag> </el-form-item>\n          <el-form-item label=\"任务名\"> <el-tag>{{dialogData.task_name}}</el-tag> </el-form-item>\n          <el-form-item label=\"命令参数\"> <el-input type=\"textarea\" :rows=\"1\" placeholder=\"null\" v-model=\"dialogData.args\" disabled></el-input> </el-form-item>\n          <el-form-item label=\"目标\"> <el-input type=\"textarea\" :rows=\"2\" placeholder=\"null\" v-model=\"dialogData.target\" disabled></el-input> </el-form-item>\n          <el-form-item label=\"账户字典\"> <el-input type=\"textarea\" :rows=\"2\" placeholder=\"null\" v-model=\"dialogData.username\" disabled></el-input> </el-form-item>\n          <el-form-item label=\"密码字典\"> <el-input type=\"textarea\" :rows=\"2\" placeholder=\"null\" v-model=\"dialogData.password\" disabled></el-input> </el-form-item>\n          <el-form-item label=\"服务\"> <el-input type=\"textarea\" :rows=\"1\" placeholder=\"null\" v-model=\"dialogData.service\" disabled></el-input> </el-form-item>\n          <el-form-item label=\"扫描周期\"> <el-tag>{{dialogData.recursion | loadTypeFilter}}</el-tag> </el-form-item>\n          <el-form-item label=\"任务状态\"> <el-tag>{{dialogData.status}}</el-tag> </el-form-item>\n          <el-form-item label=\"扫描日期\"> <el-tag>{{dialogData.date}}</el-tag> </el-form-item>\n          <el-form-item label=\"潜在威胁\"> <el-tag>{{dialogData.week_count}}个</el-tag> </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialog = false\">关 闭</el-button>\n      </span>\n    </el-dialog>\n\n    <el-table :key=\"tableKey\" v-loading=\"listLoading\" element-loading-text=\"玩命加载中\" element-loading-spinner=\"el-icon-loading\" element-loading-background=\"rgba(0, 0, 0, 0.8)\" :data=\"list\" border fit highlight-current-row style=\"width: 100%;\">\n      <el-table-column label=\"ID\" prop=\"id\" min-width=\"38px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <span>{{ row._id }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"任务名\" min-width=\"30px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <span>{{ row.task_name }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"扫描类型\" min-width=\"15px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <span>{{ row.recursion|loadTypeFilter }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"任务目标\" min-width=\"100px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <el-tag v-for=\"(item,index) in row.target\" :key=\"index\" type=\"success\" effect=\"plain\" style=\"margin-right:10px;margin-top:2px\">{{ item }}</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"备注\" min-width=\"20px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <i v-if=\"row.status=='Completed'\" class=\"el-icon-check\"  style=\"margin-left:2%\"></i>\n          <i v-else class=\"el-icon-loading\" style=\"margin-left:2%\"></i>\n          <!-- <span>{{ row.status|loadTypeFilter }}</span> -->\n          <span>{{ row.status }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"检测结果\" min-width=\"30px\" align=\"center\">\n        <template slot-scope=\"{row}\">\n          <el-rate disabled v-model=\"row.week_count\" :colors=\"colors\" show-text :texts=\"[row.week_count,row.week_count,row.week_count,row.week_count,row.week_count]\"></el-rate>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"Operation\" align=\"center\" min-width=\"40px\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"{row,$index}\">\n          <el-button type=\"primary\" size=\"mini\" class=\"operation-btn\" @click=\"handleDialog($index)\">详情</el-button>\n          <router-link :to=\"'/auth_tester/task_create?id='+row._id\"><!--dialog-->\n            <el-button type=\"success\" size=\"mini\" class=\"operation-btn\">编辑</el-button>\n          </router-link>\n          <el-button type=\"danger\" size=\"mini\" @click=\"handleDelete(row,$index)\" disabled>删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"listQuery.page\" :limit.sync=\"listQuery.limit\" @pagination=\"getList\" />\n  </div>\n</template>\n\n<script>\nimport { TaskList } from \"@/api/web_vul\"\nimport waves from \"@/directive/waves\"; // waves directive\nimport Pagination from \"@/components/Pagination\"; // secondary package based on el-pagination\n\nconst StatusOptions = [\n  { key: \"0\", display_name: \"一次\" },\n  { key: \"1\", display_name: \"每天\" },\n  { key: \"7\", display_name: \"每周\" },\n  { key: \"30\", display_name: \"每月\" },\n];\n\nconst loadTypeKeyValue = StatusOptions.reduce((acc, cur) => {\n  acc[cur.key] = cur.display_name;\n  return acc;\n}, {});\n\nexport default {\n  name: \"task_mgr\",\n  components: { Pagination },\n  directives: { waves },\n  filters: {\n    loadTypeFilter(type) {\n      return loadTypeKeyValue[type];\n    },\n  },\n  data() {\n    return {\n      tableKey: 0,\n      colors: ['#F7BA2A', '#F7BA2A', '#FF4500'],\n      list: [\n        {\n          _id: \"123\",\n          args:\"\",\n          date:\"\",\n          password:[\"123456\",\"admin\"],\n          recursion: 0,\n          service: [\"ssh\"],\n          status: \"Completed\",\n          target: [\"172.31.50.124\",\"192.168.1.141\"],\n          task_name: \"tidesec.com\",\n          username: [\"root\",\"admin\"],\n          week_count: 2\n        }\n      ],\n      dialog: false,\n      dialogData: {},\n      total: 0,\n      listLoading: false,\n      listQuery: {\n        page_no: 1, page_size: 20, info: \"\",\n      }, //页码、条数、查询字段\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    // 获取列表\n    getList() {\n      this.list = []\n      this.listLoading = true;\n      TaskList(this.listQuery).then((response) => {\n        this.list = response.data.list;\n        this.total = response.data.total;\n        setTimeout(() => { this.listLoading = false; }, 1.5 * 1000);\n      });\n    },\n    // 关键词查询\n    handleFilter() {\n      this.listQuery.page_no = 1;\n      this.getList();\n    },\n    // 删除记录\n    handleDelete(row, index) {\n      this.$confirm(\"此操作将删除该记录, 已被组装上的流水线不会因此受影响, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\",\n      }).then(() => {\n        console.log(row, index)\n        // let deleteQuery = { id: row.id };\n        // serviceDelete(deleteQuery).then((response) => {\n        //   this.$notify({ title: \"Success\", message: \"Delete Successfully\" + response.data.errmsg, type: \"success\", duration: 3000, });\n        //   this.list.splice(index, 1);\n        // });\n        this.$notify({\n          title: \"Success\", message: \"接口未实现\", type: \"success\", duration: 5000,\n        });\n      }).catch(() => {\n        this.$notify({\n          title: \"Cancel\", message: \"Delete Cancel\", type: \"info\", duration: 5000,\n        });\n      });\n    },\n    // 弹出任务详情\n    handleDialog(index){\n        let item = this.list[index]\n        this.dialogData = { _id: item._id, args: item.args, date: item.date,\n           password: item.password.toString(), recursion: item.recursion,\n           service: item.service.toString(), status: item.status,\n           target: item.target.toString(), task_name: item.task_name,\n           username: item.username.toString(), week_count: item.week_count\n        }\n        this.dialog = true\n    }\n  },\n};\n</script>\n\n<style>\n.operation-btn {\n  margin-right: 10px;\n}\n.dialog-list{\n  width: 60%;\n  word-wrap:break-word; \n  word-break:break-all;  \n  overflow: hidden;   \n}\n</style>"],"sourceRoot":"src/views/web-vul"}]}